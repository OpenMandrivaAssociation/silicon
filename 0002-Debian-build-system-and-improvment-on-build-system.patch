-        p->layout->addWidget( p->viewer );
-        p->layout->setContentsMargins( 0 , 0 , 0 , 0 );
-
-    connect( Silicon::mainWindow() , SIGNAL(resized(QSize)) , SLOT(main_resized()) );
-
-    main_resized();
-    hide();
