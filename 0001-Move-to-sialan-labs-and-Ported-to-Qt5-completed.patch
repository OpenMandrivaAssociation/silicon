diff --git a/Silicon/files/themes/Ambiance-Dream/theme.css b/Silicon/files/themes/Ambiance-Dream/theme.css
new file mode 100644
index 0000000..34de486
--- /dev/null
+++ b/Silicon/files/themes/Ambiance-Dream/theme.css
@@ -0,0 +1,220 @@
+PluginsUi{
+  background-color: qlineargradient(x1:0, y1:0.7, x2:0, y2:1.3, stop:0 #3C3B37, stop:1 #000000);
+}
+
+AboutUi{
+  background-color: qlineargradient(x1:0, y1:0.7, x2:0, y2:1.3, stop:0 #3C3B37, stop:1 #000000);
+}
+
+ApplicationsUi{
+  background-color: qlineargradient(x1:0, y1:0.7, x2:0, y2:1.3, stop:0 #3C3B37, stop:1 #000000);
+}
+
+ConfigureUi{
+  background-color: qlineargradient(x1:0, y1:0.7, x2:0, y2:1.3, stop:0 #3C3B37, stop:1 #000000);
+}
+
+STabBar:item{
+  border-style:solid;
+  color: #F0EBE2;
+  border-radius: 3px;
+}
+
+STabBar:item:hover{
+  background-color: $(HIGHLIGHT)%23;
+  color: #F0EBE2;
+  border-radius: 3px;
+}
+
+STabBar:item:pressed{
+  background-color: $(HIGHLIGHT)%23;
+  color: #F0EBE2;
+  border-radius: 3px;
+}
+
+STabBar:item:checked{
+  background-color: $(HIGHLIGHT)%37;
+  color: #F0EBE2;
+  border-radius: 3px;
+}
+
+
+
+STabBar:TabBar:Arrow:!hover{
+  border-style: solid;
+  color: #F0EBE2;
+}
+
+STabBar:TabBar:Arrow:hover{
+  border: 1px solid $(HIGHLIGHT);
+  border-radius: 3px;
+  color: #F0EBE2;
+}
+
+STabBar:TabBar:Arrow:pressed{
+  border-color: $(HIGHLIGHT);
+  border-style: inset;
+  color: #F0EBE2;
+}
+
+STabBar:TabBar:Back{
+  background-color: $(WINDOW)*0.2;
+}
+
+STabBar:TabBar:Frame{
+  border: 2px solid qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 $(WINDOW)*0.3, stop:1 transparent);
+  border-radius: 3px;
+}
+
+STabBar:Background{
+  background-color: qlineargradient(x1:0, y1:0.7, x2:0, y2:1.3, stop:0 #3C3B37, stop:1 #000000);
+}
+
+
+
+CatWidget:Category{
+  background-image: url($(:left-brok.png));
+  background-position: bottom left;
+  background-repeat: no-repeat;
+  alternate-background-color: transparent;
+  background-color: qradialgradient(cx:0, cy:0, radius: 1,fx:0.5, fy:0.5, stop:0 $(HIGHLIGHT), stop:1 $(HIGHLIGHT)/2);
+  border-style: solid;
+  show-decoration-selected: 1;
+  min-width:137px;
+  max-width:137px;
+}
+
+CatWidget:Category:item:hover {
+  background-color: qlineargradient(x1:0, y1: 0, x2:5, y2:0, stop:0 transparent, stop:1 #ffffff );
+  color: $(HIGHLIGHTED_TEXT);
+}
+
+CatWidget:Category:item:!hover {
+  background-color: transparent;
+  color: $(HIGHLIGHTED_TEXT);
+}
+
+CatWidget:Category:item:selected {
+  background-image: url($(:selected.png));
+  background-color: qlineargradient(x1:0.1, y1: 0, x2:2, y2:0, stop:0 transparent, stop:1 #ffffff );
+  selection-color: $(HIGHLIGHTED_TEXT);
+  background-position: center right;
+  background-repeat: no-repeat;
+}
+
+CatWidget:Background{
+  background-color: #ffffff;
+}
+
+CatWidget:List{
+  background-image: solid;
+  border-style: solid;
+  show-decoration-selected: 1;
+}
+
+CatWidget:SearchBar:button:!hover{
+  border: 1px solid #dedede;
+}
+
+CatWidget:SearchBar:button:hover{
+  border: 1px solid $(HIGHLIGHT);
+}
+
+CatWidget:SearchBar:button:pressed{
+  border-color: $(HIGHLIGHT);
+  border-right-color: #dedede;
+  border-style: inset;
+}
+
+CatWidget:SearchBar:widget:QLineEdit{
+  border: 1px solid #dedede;
+  border-left-color: solid;
+}
+
+CatWidget:ViewBar:button:!hover{
+  border: 1px solid #dedede;
+  border-right-color: solid;
+}
+
+CatWidget:ViewBar:button:hover{
+  border: 1px solid $(HIGHLIGHT);
+}
+
+CatWidget:ViewBar:button:pressed{
+  border-color: $(HIGHLIGHT);
+  border-right-color: solid;
+  border-style: inset;
+}
+
+
+
+CatWidget:ViewBar:widget:QPushButton{
+}
+
+CatWidget:ViewBar:widget:QFrame{
+  border: 1px solid #dedede;
+  border-right-color: solid;
+}
+
+SInfoWidget{
+  background-image: url($(:down.png));
+  background-position: bottom right;
+  background-repeat: no-repeat;
+  background-color: qradialgradient(cx:0.5, cy:0, radius: 1,fx:1, fy:0.5, stop:0 $(HIGHLIGHT), stop:1 $(HIGHLIGHT)/2);
+  color: $(HIGHLIGHTED_TEXT);
+}
+
+SContextButton:!hover{
+  border: 1px solid $(WINDOW)*0.4;
+  border-radius: 3px;
+}
+
+SContextButton:hover{
+  border: 1px solid $(HIGHLIGHT);
+  border-radius: 3px;
+}
+
+SContextButton:pressed{
+  border-color: $(HIGHLIGHT);
+  border-style: inset;
+}
+
+SMenuButton:!hover{
+  border: 1px solid $(WINDOW)*0.4;
+  border-radius: 3px;
+}
+
+SMenuButton:hover{
+  border: 1px solid $(HIGHLIGHT);
+  border-radius: 3px;
+}
+
+SMenuButton:pressed{
+  border-color: $(HIGHLIGHT);
+  border-style: inset;
+}
+
+SMenu:topLeft{
+  background-color: qlineargradient(x1: 2, y1: 3, x2:0, y2:0, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
+SMenu:top{
+  background-color: qlineargradient(x1: 0, y1: 2, x2:0, y2:0, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
+SMenu:topRight{
+  background-color: qlineargradient(x1:-1, y1: 3, x2:1, y2:0, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
+SMenu:left{
+  background-color: qlineargradient(x1: 2, y1: 0, x2:0, y2:0, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
+SMenu:right{
+  background-color: qlineargradient(x1:-1, y1: 0, x2:1, y2:0, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
+SMenu:bottomLeft{
+  background-color: qlineargradient(x1: 2, y1:-2, x2:0, y2:1, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
+SMenu:bottom{
+  background-color: qlineargradient(x1: 0, y1:-1, x2:0, y2:1, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
+SMenu:bottomRight{
+  background-color: qlineargradient(x1:-1, y1:-2, x2:1, y2:1, stop:0 $(HIGHLIGHT), stop:1 transparent);
+}
